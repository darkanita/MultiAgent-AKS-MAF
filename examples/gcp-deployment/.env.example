# Copy this file to .env and customize
# DO NOT commit .env to version control!

# GCP Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1
SERVICE_NAME=adk-agent

# Agent Configuration
AGENT_NAME=gcp-adk-agent
AGENT_DESCRIPTION=My ADK agent running in Google Cloud
AGENT_CAPABILITIES=search,booking,recommendations
AGENT_KEYWORDS=travel,hotels,flights

# Security (Optional)
# Uncomment to enable API key authentication
# API_KEY=your-secret-api-key-here

# ADK-Specific Configuration
# Add your ADK/Vertex AI configuration here
# VERTEX_AI_PROJECT=your-project
# VERTEX_AI_LOCATION=us-central1
# VERTEX_AI_MODEL=gemini-pro
